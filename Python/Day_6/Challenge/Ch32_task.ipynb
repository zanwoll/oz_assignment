   "cell_type": "code",
   "source": [
    "def longer_string(str1, str2):\n",
    "    if len(str1) > len(str2):\n",
    "        return str2\n",
    "    else:\n",
    "        return str1\n",
    "\n",
    "print(longer_string(\"hello\", \"world\"))"
   ],
