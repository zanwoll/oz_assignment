   "cell_type": "code",
   "source": [
    "def print_lengths(str):\n",
    "    for str in str:\n",
    "        print(f\"{str}의 길이: {len(str)}\")\n",
    "str_words = [\"안녕하세요\", \"반갑습니다\", \"잘 부탁드려요\"]\n",
    "print_lengths(str_words)"
