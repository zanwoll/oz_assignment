   },
   "cell_type": "code",
   "source": [
    "def is_even(number):\n",
    "    if number % 2 == 0:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "print(is_even(2)) # 숫자 변경 시 결과 값이 달라짐( True 또는 False )"
   ],
