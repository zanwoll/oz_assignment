   "cell_type": "code",
   "source": [
    "def factorial(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    return n * factorial(n - 1)\n",
    "\n",
    "print(factorial(5)) # ( 5 * 4 * 3 * 2 * 1 ) = 120\n"
   ],
